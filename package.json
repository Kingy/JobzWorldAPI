{
  "name": "jobzworld-api",
  "version": "1.0.0",
  "description": "Jobzworld API - Video-first job matching platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "vercel-build": "npm run build",
    "test:watch": "jest --watch",
    "migrate:up": "ts-node src/database/migrate.ts up",
    "migrate:down": "ts-node src/database/migrate.ts down",
    "seed": "ts-node src/database/seed.ts"
  },
  "keywords": ["jobs", "recruitment", "video", "ai", "matching"],
  "author": "Jobzworld Team",
  "license": "MIT",
  "dependencies": {
    "express": "*",
    "cors": "*",
    "helmet": "*",
    "compression": "*",
    "morgan": "*",
    "dotenv": "*",
    "bcrypt": "*",
    "jsonwebtoken": "*",
    "joi": "*",
    "pg": "*",
    "nodemailer": "*",
    "@aws-sdk/client-s3": "*",
    "@aws-sdk/s3-request-presigner": "*",
    "multer": "*",
    "uuid": "*",
    "rate-limiter-flexible": "*",
    "typescript": "*",
    "ts-node": "*",
    "@types/node": "*"
  },
  "devDependencies": {
    "@types/node": "*",
    "@types/express": "*",
    "@types/cors": "*",
    "@types/compression": "*",
    "@types/morgan": "*",
    "@types/bcrypt": "*",
    "@types/jsonwebtoken": "*",
    "@types/pg": "*",
    "@types/nodemailer": "*",
    "@types/multer": "*",
    "@types/uuid": "*",
    "@types/jest": "*",
    "typescript": "*",
    "nodemon": "*",
    "ts-node": "*",
    "jest": "*",
    "ts-jest": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*"
  }
}