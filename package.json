{
  "name": "jobzworld-api",
  "version": "1.0.0",
  "description": "Jobzworld API - Video-first job matching platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate:up": "node dist/database/migrate.js up",
    "migrate:down": "node dist/database/migrate.js down",
    "migrate:create": "node dist/database/migrate.js create",
    "seed": "node dist/database/seed.js"
  },
  "keywords": [
    "jobs",
    "recruitment",
    "video",
    "ai",
    "matching"
  ],
  "author": "Jobzworld Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "*",
    "@aws-sdk/s3-request-presigner": "*",
    "bcrypt": "*",
    "compression": "*",
    "cors": "*",
    "dotenv": "*",
    "express": "*",
    "helmet": "*",
    "joi": "*",
    "jsonwebtoken": "*",
    "morgan": "*",
    "multer": "*",
    "nodemailer": "*",
    "pg": "*",
    "rate-limiter-flexible": "*",
    "uuid": "*"
  },
  "devDependencies": {
    "@types/bcrypt": "*",
    "@types/compression": "*",
    "@types/cors": "*",
    "@types/express": "*",
    "@types/jest": "*",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "*",
    "@types/multer": "*",
    "@types/node": "*",
    "@types/nodemailer": "*",
    "@types/pg": "*",
    "@types/uuid": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "jest": "*",
    "nodemon": "*",
    "ts-jest": "*",
    "ts-node": "*",
    "typescript": "*"
  }
}
